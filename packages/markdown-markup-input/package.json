{
  "name": "markdown-markup-input",
  "version": "1.0.2",
  "description": "An embedded markdown text editor based on stackedit.",
  "module": "dist/markdown-markup-input.umd.js",
  "unpkg": "dist/markdown-markup-input.umd.min.js",
  "jsdelivr": "dist/markdown-markup-input.umd.min.js",
  "repository": {
    "url": "https://github.com/AeliGo/md-text.git",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/AeliGo/md-text/issues"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "serve": "vue-cli-service serve --port 8089",
    "build": "vue-cli-service build --target lib --name markdown-markup-input",
    "lint": "vue-cli-service lint"
  },
  "author": "zoey",
  "license": "MIT",
  "dependencies": {
    "vue": "^2.6.10",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.9.2",
    "@vue/cli-service": "^5.0.8",
    "diff-match-patch": "^1.0.4",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "lovue": "^3.1.1",
    "markdown-it": "^9.0.1",
    "markdown-it-abbr": "^1.0.4",
    "markdown-it-deflist": "^2.0.3",
    "markdown-it-footnote": "^3.0.2",
    "markdown-it-pandoc-renderer": "^1.1.3",
    "markdown-it-sub": "^1.0.0",
    "markdown-it-sup": "^1.0.0",
    "prismjs": "^1.17.1",
    "raw-loader": "^3.1.0",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "es6": true,
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off",
      "no-undef": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "browserslist": [
    "Chrome >= 70",
    "Firefox >= 65",
    "Safari >= 11"
  ]
}
